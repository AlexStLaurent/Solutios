@*@model IEnumerable<Solutios.Models.subExpense>*@
<form>
	<table class="table" id="tableSubExpenses">
		<thead>
			<tr>
				<th>
					Dépense@*@Html.DisplayNameFor(model => model.name)*@
				</th>
				<th>
					Coûts@*@Html.DisplayNameFor(model => model.cost)*@
				</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model)
				{
			<tr id="tablerow0">
				<td>
					<input class="form-control" name="subName + 0" type="text" placeholder="Entrer un nom..." />@*@Html.DisplayFor(modelItem => item.id)*@
				</td>
				<td>
					<input  class="form-control" name="subCost" type="number" placeholder="Entrer un montant..." />@*@Html.DisplayFor(modelItem => item.name)*@
				</td>
				
			</tr>

			 } 
		</tbody>
	</table>
	<p>
		<button class="btn btn-primary text-white" style="width:100%" id="add" value="Ajouter">Ajouter</button>
	</p>
</form>

<script src="~/js/ExpenseTable.js"></script>